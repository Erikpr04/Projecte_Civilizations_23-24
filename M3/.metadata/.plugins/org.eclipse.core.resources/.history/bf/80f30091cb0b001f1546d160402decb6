package classes;

public class dc_classes {
	
	 Civilization civilization = new Civilization();

	
	
	public static void new_smithy() {
		civilization.setSmithy(civilization.getSmithy()+1);

	};
	
	public static void new_church() {
		civilization.setChurch(civilization.getChurch()+1);

	};
	
	
	
	

}
